{
  "root": "./pages",
  "title": "Manual de versionamento de código",
  "cover":"images/cover.jpg",
  "description": "Manual com o conhecimento institucional de versionamento de código", 
  "language": "pt",
  "structure": {
    "readme": "apresentacao.md",
    "summary": "SUMMARY.md"
  },
  "plugins":["-sharing","-lunr","github","sharing-plus","download-pdf-link","-search", "search-pro","fontsettings"
              ,"mermaid-gb3","splitter","anchor-navigation-ex","code","tbfed-pagefooter"],
  "pluginsConfig": {
    "download-pdf-link": {
      "base": "https://github.com/GeovanneAtanazio/manual_versionamento_de_codigo/blob/main/Manual_Versionamento_Codigo.pdf"
      },
    "fontsettings": {
      "theme": "sepia"
    },
    "sharing": {
      "douban":false,
      "facebook":false,
      "google":false,
      "line":false,
      "qq":false,
      "qzone":false,
      "weibo":false,
      "whatsapp":false,
      "all": ["twitter","linkedin","whatsapp","facebook"]
    },
    "github":{
      "url": "https://github.com/GeovanneAtanazio/manual_versionamento_de_codigo"
    },
    "tbfed-pagefooter": {
      "copyright":"Copyright &copy 2021 Geovanne Atanazio",
      "modify_label": "Arquivo revisado pela última vez em",
      "modify_format": "DD/MM/YYYY"
